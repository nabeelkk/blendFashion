    <main class="main-wrap">
        <%- include('../partials/adminNav') %>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    
                </div>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            <div class="form-check ms-2">
                                <input class="form-check-input" type="checkbox" value="">
                            </div>
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <select class="form-select">
                                <option selected>All category</option>
                                
                            </select>
                        </div>
                        <div class="col-md-2 col-6">
                            <input type="date" value="02.05.2021" class="form-control">
                        </div>
                        <div class="col-md-2 col-6">
                            <a class="menu" href="addProducts"> <i style="font-size: 30px;" class="icon material-icons md-add_box "></i>
                               <span style="font-size: 15px;">Add Product</span>
                            </a>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                        <% product.forEach((product) => { %>
                            <article class="itemlist">
                                <div class="row align-items-center">
                                    <div class="col col-check flex-grow-0">
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox">
                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                                        <a class="itemside" href="/admin/productDetails/<%= product._id %>">
                                            <div class="left">
                                             <img src="https://res.cloudinary.com/<%=cloudName  %>/image/upload/<%= product.images[0] %>" class="img-sm img-thumbnail" alt="Product-image"> 
                                            </div>
                                            <div class="info">
                                                <h6 class="mb-0"><%= product.name %></h6>
                                            </div>
                                        </a>
                                    </div>
                                    <div class="col-lg-2 col-sm-2 col-4 col-price"> <span><%= product.price.seller %></span> </div>
                                    <div class="col-lg-2 col-sm-2 col-4 col-status"> 
                                        
                                    </div>
                                    <!-- <div class="col-lg-1 col-sm-2 col-4 col-date">
                                        <span>02.11.2021</span>
                                    </div> -->
                                    <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                        <a href="/admin/editProduct/<%= product._id %>"><button  class="btn  btn-brand rounded font-sm mt-15">Edit</button></a>
                                        <a href="/admin/deleteProduct?id=<%= product._id %>" onclick="return confirm('Are you sure you want to delete this product?')" ><button class="btn  btn-brand rounded font-sm mt-15">
                                            Delete
                                        </button> 
                                        </a>
                                    </div>
                                </div> <!-- row .// -->
                            </article>
                     <% }) %>
                </div> 
            </div> <!-- card end// -->
            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-start">
                        <% for( let i = 1; i <= totalPage; i++ ) { %>
                        <li class="page-item <%= parseInt(page) === i ? 'active':'' %>">
                            <a class="page-link" href="/admin/productList?page=<%= i %>&search=<%= search %>"><%= i %></a></li>
                        <% } %>
                    </ul>
                </nav>
            </div>
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
