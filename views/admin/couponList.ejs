        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupons</h2>
                </div>
                <div>
                    <a href="addCoupon" class="btn btn-md rounded font-sm">Create New</a>
                </div>                
            </div>
            <div class="card mb-4">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Coupon code</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Discount</th>
                                    <th scope="col">Min Purchase</th>
                                    <th scope="col">Coupon Expirey</th>
                                    <th scope="col">Expire Status</th>
                                    <th scope="col">Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% coupon.forEach(el =>{%> 
                                <tr>
                                    <td><%= el.couponcode %></td>
                                    <td><b><%= el.name %></b></td>
                                    <td><%= el.discount %>%</td>
                                    <td>â‚¹<%= el.minPurchase %></td>
                                    <td><%= el.expiryDate.toISOString().split('T')[0]  %></td>
                                    <% 
                                        const expiryDate = new Date(el.expiryDate);
                                        const currentDate = new Date();
                                        const timeDiff = expiryDate - currentDate;
                                        const daysLeft = Math.ceil(timeDiff / (1000 * 60 * 60 * 24));
                                    %>
                                   <% if (daysLeft <= 0) { %>
                                        <td><span class="badge rounded-pill alert-danger">Expired</span></td>
                                        <% } else { %>
                                        <td><span class="badge rounded-pill alert-success"><%= daysLeft %> Days left</span></td>
                                        <% } %>
                                    
                                    <% if(el.isActive == true){%>
                                    <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    
                                    <%}else{%>
                                        <td><span class="badge rounded-pill alert-danger">Inactive</span></td>
                                        <%}%>
                                    <td class="text-end">
                                        <a href="/admin/editCoupon?id=<%= el._id %>" class="btn btn-md rounded font-sm">Edit</a>
                                       
                                        <a href="/admin/blockCoupon?id=<%= el._id %>" class="btn btn-md rounded font-sm">
                                            <% if(el.isActive == true){%>
                                                Block
                                            <%}else{%> 
                                                Unblock
                                            <%}%>  
                                        </a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div> 
                </div>
            </div> 
