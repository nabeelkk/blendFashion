    <main class="main mb-50">
        <section class="pt-150 ">
            <div class="container">
                <div class="row">
                    <div class="col-lg-10 m-auto">
                        <div class="row">
                            <div class="col-lg-1"></div>
                            <div class="col-lg-1"></div>
                            <div class="col-lg-1"></div>
                            <div class="col-lg-1"></div>
                            <div class="col-lg-5">
                                <div class="login_wrap widget-taber-content p-30 background-white border-radius-10 mb-md-5 mb-lg-0 mb-sm-5">
                                    <div class="padding_eight_all bg-white">
                                       <% if (error) { %>
                                        <div><p style="font-size: 15px;color: red; margin-bottom: 10px;"><%= error %></p></div>
                                       <% } %>
                                        <div class="heading_s1">
                                            <h3 class="mb-30">Login</h3>
                                        </div>
                                        <form method="post">
                                            <div class="form-group">
                                                <input type="email" required="" id="Lemail" name="email" placeholder="Your Email">
                                                <p id="error1" style="display: none; color: red;"></p>
                                            </div>
                                            <div class="form-group">
                                                <input required="" type="password" id="Lpass" name="password" placeholder="Password">
                                                <p id="error2" style="display: none; color: red;"></p>
                                            </div>
                                            <div class="login_footer form-group">
                                                <div class="chek-form">
                                                    <div class="custome-checkbox">
                                                        <input class="form-check-input" type="checkbox" name="checkbox" id="exampleCheckbox1" value="">
                                                        
                                                    </div>
                                                </div>
                                                <a class="text-muted" href="/forgotpassword">Forgot password?</a>
                                            </div>
                                            <br>
                                        <%if(locals.message){%>
                                            <p style="color: red;"><%=locals.message%></p>
                                        <%}%>
                                        <div class="form-group">
                                            <button type="submit" class="btn btn-fill-out btn-block hover-up" name="home">Log in</button>
                                        </div>
                                        <div class="d-grid gap-3 mb-4">
                                            <a href="/user/google" class="btn w-100 btn-light font-sm">
                                                <svg aria-hidden="true" class="icon-svg" width="20" height="20" viewBox="0 0 20 20">
                                                    <path d="M16.51 8H8.98v3h4.3c-.18 1-.74 1.48-1.6 2.04v2.01h2.6a7.8 7.8 0 002.38-5.88c0-.57-.05-.66-.15-1.18z" fill="#4285F4"></path>
                                                    <path d="M8.98 17c2.16 0 3.97-.72 5.3-1.94l-2.6-2a4.8 4.8 0 01-7.18-2.54H1.83v2.07A8 8 0 008.98 17z" fill="#34A853"></path>
                                                    <path d="M4.5 10.52a4.8 4.8 0 010-3.04V5.41H1.83a8 8 0 000 7.18l2.67-2.07z" fill="#FBBC05"></path>
                                                    <path d="M8.98 4.18c1.17 0 2.23.4 3.06 1.2l2.3-2.3A8 8 0 001.83 5.4L4.5 7.49a4.77 4.77 0 014.48-3.3z" fill="#EA4335"></path>
                                                </svg> Sign in using Google
                                            </a>    
                                        </div>
                                        
                                            
                                            <div class="text-muted text-center">Don't have an account? <a href="/signup">Sign up now</a></div>
